authentik:
  secret_key: "${authentik_secret_key}"
  # This sends anonymous usage-data, stack traces on errors and
  # performance data to sentry.io, and is fully opt-in
  error_reporting:
    enabled: true
  postgresql:
    password: "${authentik_postgres_password}"

server:
  ingress:
    enabled: false

postgresql:
  enabled: true
  auth:
    password: "${authentik_postgres_password}"
redis:
  enabled: true
